<script src="/dash/assets/vendor/charts/c3charts/d3-5.4.0.min.js" ></script >
<script src="/dash/assets/vendor/charts/c3charts/C3chartjs.js" ></script >
<script src="/dash/assets/libs/js/dashboard-ecommerce.js" ></script >

<script type="application/javascript" >
    $(function () {



        var chart = c3.generate({
            bindto: "#bar",
            color: {pattern: ["#5969ff", "#ff407b", "#25d5f2", "#ffc750"]},
            data: {
                // iris data from R
                columns: [

                    {% for key,sum in sum_ar_bar %}
                        ['{{ key }} {{ (sum/(vir_bar/100))|round }}%',{{ sum }}],
                    {% endfor%}
                    ],
                type: 'pie',


            }
        });
        var chart = c3.generate({
            bindto: "#kitch",
            color: {pattern: ["#5969ff", "#ff407b", "#25d5f2", "#ffc750"]},
            data: {
                // iris data from R
                columns: [

                    {% for key,sum in sum_ar_kitch %}
                    ['{{ key }} {{ (sum/(vir_kitch/100))|round }}%',{{ sum }}],
                    {% endfor%}
                ],
                type: 'pie',


            }
        });
        var chart = c3.generate({
            bindto: "#type",
            color: {pattern: ["#5969ff", "#ff407b", "#25d5f2", "#ffc750"]},
            data: {
                // iris data from R
                columns: [


                    ['бар {{ (vir_bar/((vir_bar+vir_kitch)/100))|round(1) }}%',{{ vir_bar }}],
                    ['Кухня {{ (vir_kitch/((vir_bar+vir_kitch)/100))|round(1) }}%',{{ vir_kitch }}],
                ],
                type: 'pie',


            }
        });

    })
    $('table').tablesort();

</script >
</body >

</html >